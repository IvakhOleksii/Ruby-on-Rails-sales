<p id="notice"><%= notice %></p>
<table class="table" data-toggle="table" data-pagination="true" data-side-pagination="server"
       data-url="requests.json" data-page-list="[25, 50]" data-search="true" id="request-table"
       data-show-refresh="true" data-id-field="id">
  <thead>
    <tr>
      <th data-field="created_at" data-formatter="dateFormatter"></th>
      <th data-field="id" data-formatter="hideButton"></th>
      <th data-field="user" data-formatter="userFormatter"></th>
      <th data-field="is_first_time" data-formatter="firstFormatter">1<sup>st</sup></th>
      <th data-field="gender">Sex</th>
      <th data-field="has_color" data-formatter="colorFormatter">Color?</th>
      <th data-field="has_cover_up" data-formatter="coverFormatter"></th>
      <th data-field="position">Position</th>
      <th data-formatter="cartButtons"></th>
    </tr>
  </thead>

  <tbody>
  </tbody>
</table>

<br>
